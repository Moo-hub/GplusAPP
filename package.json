{
  "name": "gplusapp",
  "version": "1.0.0",
  "description": "G+ App for recycling management and environmental impact tracking",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:basic": "vitest run basic.test.js basic-react-simple.test.jsx react-element.test.jsx react-component.test.jsx",
    "test:ui": "vitest --ui",
    "test:components": "vitest run --config vitest.components.config.js",
    "test:components:watch": "vitest --config vitest.components.config.js",
    "test:component:file": "vitest run --config vitest.components.config.js",
    "test:components:coverage": "vitest run --config vitest.components.config.js --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:3000 cypress:open",
    "cy:run": "start-server-and-test dev http://localhost:3000 cypress:run",
    "cy:html-report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-html-config.json",
    "cy:dashboard": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-dashboard-config.json",
    "cy:visual": "cross-env VISUAL_TESTING=true cypress run --env VISUAL_TESTING=true --reporter cypress-multi-reporters --reporter-options configFile=reporter-visual-config.json",
    "cy:performance": "cross-env PERFORMANCE_MONITORING=true cypress run --spec cypress/e2e/performance*.cy.js --env PERFORMANCE_MONITORING=true",
    "cy:all-reports": "npm run cy:html-report && npm run cy:dashboard && npm run cy:visual",
    "cy:run-parallel": "cypress-parallel -s cy:run -t 4 -d cypress/e2e/**/*.cy.js",
    "open:reports": "start cypress/reports/html/test-results.html",
    "test:performance": "node scripts/run-performance-tests.js",
    "test:performance:report": "cross-env PERFORMANCE_MONITORING=true node scripts/run-performance-tests.js",
    "test:performance:update-baseline": "cross-env PERFORMANCE_MONITORING=true UPDATE_BASELINE=true node scripts/run-performance-tests.js",
    "codemod:act": "jscodeshift -t tools/codemods/transform-act-imports.js frontend/src frontend/src/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moo-hub/GplusAPP.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Moo-hub/GplusAPP/issues"
  },
  "homepage": "https://github.com/Moo-hub/GplusAPP#readme",
  "devDependencies": {
    "@ant-design/icons": "^6.0.2",
    "@cypress/code-coverage": "^3.12.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
  "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "antd": "^5.27.4",
    "cypress": "^12.0.0",
    "cypress-axe": "^1.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^2.2.5",
    "cypress-network-idle": "^1.14.2",
    "fake-indexeddb": "^3.1.8",
    "istanbul-lib-coverage": "^3.2.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^21.1.0",
    "msw": "^2.11.3",
    "node-fetch": "^2.6.7",
    "nyc": "^15.1.0",
    "react-icons": "^5.5.0",
    "start-server-and-test": "^2.0.0",
    "styled-components": "^6.1.19",
  "vite": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "dependencies": {
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
  "@tanstack/react-query": "^5.8.4",
  "@tanstack/react-query-devtools": "^5.8.4",
    "axios": "^0.27.2",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    
    "react-calendar": "^4.6.0",
    
    "react-i18next": "^13.2.2",
    "react-query": "^3.39.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
